{
  "name": "decentraland",
  "version": "3.4.4",
  "description": "Decentraland CLI developer tool.",
  "bin": {
    "dcl": "dist/cli.js"
  },
  "files": [
    "bin",
    "dist",
    "samples",
    "abi"
  ],
  "scripts": {
    "start": "npm run watch",
    "watch": "npm run cli:watch",
    "cli:build": "webpack",
    "cli:watch": "webpack --watch",
    "linker:build": "decentraland-compiler build.linker.json",
    "linker:watch": "decentraland-compiler build.linker.json --watch",
    "scripts:build": "decentraland-compiler build.json",
    "build": "npm run cli:build && npm run linker:build && npm run scripts:build",
    "lint": "tslint -e 'samples/**/*' -e '*.json' -c tslint.json 'src/**/*.ts'",
    "test": "FORCE_COLOR=1 ava",
    "test:dry": "FORCE_COLOR=1 ava --update-snapshots",
    "test:ci": "FORCE_COLOR=1 ava -v --fail-fast --match='!*E2E*' --match='!*Decentraland.deploy()*'",
    "test:win": "set FORCE_COLOR=1 ava -v --fail-fast --match=\"!*E2E*\" --match=\"!*Decentraland.deploy()*\"",
    "publish:cli": "node ./scripts/npmPublish.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "*.ts": [
        "tslint -c tslint.json",
        "prettier --single-quote --no-semi --write",
        "git add"
      ]
    },
    "ignore": [
      "samples/**/*.ts"
    ]
  },
  "repository": "decentraland/cli",
  "keywords": [
    "decentraland",
    "cli",
    "dcl",
    "mana",
    "land"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/decentraland/cli",
  "devDependencies": {
    "@babel/runtime": "^7.10.0",
    "@types/analytics-node": "3.1.3",
    "@types/archiver": "^3.1.0",
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.1",
    "@types/inquirer": "6.5.0",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/node": "^14.0.5",
    "@types/opn": "^5.5.0",
    "@types/ora": "^3.2.0",
    "@types/puppeteer": "^3.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/request": "^2.48.5",
    "@types/sinon": "^9.0.4",
    "@types/ws": "^7.2.4",
    "@walletconnect/core": "^1.0.0-beta.47",
    "@walletconnect/types": "^1.0.0-beta.47",
    "@walletconnect/utils": "^1.0.0-beta.47",
    "analytics-node": "^3.3.0",
    "archiver": "^4.0.1",
    "arg": "^4.1.3",
    "ava": "^1.4.1",
    "chalk": "^4.0.0",
    "chokidar": "^3.4.0",
    "cors": "^2.8.5",
    "cross-spawn": "^7.0.3",
    "dcl-catalyst-client": "^1.2.5",
    "dcl-tslint-config-standard": "^2.0.0",
    "decentraland-dapps": "7.6.1",
    "decentraland-eth": "^8.18.2",
    "decentraland-rpc": "^3.1.8",
    "docker-names": "^1.1.1",
    "eth-connect": "^4.0.2",
    "ethers": "^4.0.47",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "git-rev-sync": "^2.0.0",
    "glob": "^7.1.6",
    "google-protobuf": "^3.12.1",
    "husky": "^4.2.5",
    "ignore": "^5.1.6",
    "inquirer": "^7.1.0",
    "interface-datastore": "^1.0.2",
    "isomorphic-fetch": "^2.2.1",
    "lint-staged": "^10.2.6",
    "mkdirp": "^1.0.4",
    "opn": "^6.0.0",
    "ora": "^4.0.4",
    "package-json": "^6.5.0",
    "portfinder": "^1.0.26",
    "prettier": "^2.0.5",
    "proxyquire": "^2.1.3",
    "puppeteer": "^3.1.0",
    "qrcode-terminal": "^0.12.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.5",
