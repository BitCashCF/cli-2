    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "tslint-language-service": "^0.9.9",
    "typesafe-actions": "^5.1.0",
    "typescript": "^3.9.3",
    "uuid": "^8.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "wildcards": "^1.0.2",
    "ws": "^7.3.0"
  },
  "dependencies": {
    "decentraland-ui": "^2.20.4"
  }
}
